{
  "manifest_version": 2,
  "name": "Tooltip Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://en.wikipedia.org/*"],
      "js": ["jquery.js", "jquery-ui.js", "content.js"],
      "css": ["jquery-ui.css", "content.css"]
    }
  ],
  "permissions": ["activeTab"]
}